{
  "name": "Avalon",
  "description": "A Vue.js project",
  "author": "calibur.tv",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development node server",
    "start": "cross-env NODE_ENV=staging node server",
    "analyzer": "cross-env NODE_ENV=staging webpack --config build/webpack.analyzer.config.js --progress --hide-modules",
    "staging": "rimraf dist && npm run staging:client && npm run staging:server",
    "staging:client": "cross-env NODE_ENV=staging webpack --config build/webpack.client.config.js --progress --hide-modules",
    "staging:server": "cross-env NODE_ENV=staging webpack --config build/webpack.server.config.js --progress --hide-modules",
    "production": "rimraf dist && npm run production:client && npm run production:server",
    "production:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "production:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "test": "mocha --require babel-register"
  },
