{
    "vetur.format.defaultFormatterOptions": {
        // vue-html格式化方式
        "js-beautify-html": {
            // attr自动换行对齐
            "wrap_attributes": "force-aligned"
        }
    },
    // 全局搜索忽略文件
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/dist": true,
        "**/theme": true
    }
}